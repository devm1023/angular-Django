{
    "compilerOptions": {
        "outDir": "site_assets",
        "target": "es5",
        "module": "commonjs",
        "preserveConstEnums": true,
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules"
    ]
}
